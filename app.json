{
  "expo": {
    "scheme":"learningapp",
    "name": "Learning",
    "slug": "Learning",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/logo_demo.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/img/logo_demo.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#000000",
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/img/logo_demo.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    }, 
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
